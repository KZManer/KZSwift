#pod 'FWPopupView'               #弹窗
# pod 'JXSegmentedView'           #分页
pod 'KeychainAccess', '~>4.2.2' #钥匙串
pod 'Kingfisher', '~> 7.0'      #图片框架
pod 'KMNavigationBarTransition' #管理导航栏转场
# pod 'MJRefresh', '~> 3.1.14.1'  #刷新框架
pod 'SnapKit', '~>5.0'          #自动布局框架
